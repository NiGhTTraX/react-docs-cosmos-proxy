{
  "name": "react-cosmos-docs-proxy",
  "version": "1.0.0",
  "description": "A React Cosmos proxy for showing component docs using react-docgen",
  "keywords": [
    "react",
    "cosmos",
    "playground",
    "docs",
    "docgen"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:NiGhTTraX/react-docs-cosmos-proxy.git"
  },
  "author": "Andrei Picus",
  "license": "MIT",
  "module": "./src/docs-proxy.jsx",
  "scripts": {
    "test": "./tests/run.sh",
    "test:unit": "NODE_ENV=unit nyc npm run _mocha",
    "test:unit:watch": "NODE_ENV=unit npm run _mocha -- --opts tests/unit/mocha.watch.opts",
    "_mocha": "mocha tests/unit/helpers/index.node.js 'tests/unit/**/*.spec.js'",
    "test:unit:debug": "NODE_ENV=unit webpack --bail -w --config ./tests/unit/webpack.config.unit.js",
    "test:acceptance": "./tests/acceptance/run.sh",
    "test:acceptance:debug": "./tests/acceptance/debug.sh",
    "_test:acceptance": "mocha --opts tests/acceptance/mocha.opts tests/acceptance/setup.js --recursive tests/acceptance/specs",
    "_test:acceptance:debug": "mocha --opts tests/acceptance/mocha.debug.opts tests/acceptance/setup.js --recursive tests/acceptance/specs",
    "start": "./start.sh",
    "playground": "cosmos --config playground/cosmos.config.js",
    "lint": "npm run lint:files -- .",
    "lint:files": "eslint --ext js --ext jsx",
    "postinstall": "./tools/install-git-hooks.sh"
  },
  "dependencies": {
    "classnames": "~2.2.5",
    "prop-types": "~15.6.0",
    "react-cosmos-utils": "~2.0.0",
    "reset-css": "~2.2.1"
  },
  "peerDependencies": {
    "babel-plugin-react-docgen": "^1.8.1",
    "react": "^16.0.0",
    "react-cosmos-webpack": "^2.1.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-core": "~6.26.0",
    "babel-eslint": "~8.0.1",
    "babel-loader": "~7.1.0",
    "babel-plugin-istanbul": "~4.1.1",
    "babel-plugin-module-resolver": "~3.0.0",
    "babel-plugin-react-docgen": "^1.8.1",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-object-rest-spread": "~6.26.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.6.0",
    "babel-preset-react": "~6.24.1",
    "babel-preset-react-hmre": "~1.1.1",
    "babel-register": "~6.26.0",
    "chai": "~4.1.0",
    "css-loader": "~0.28.0",
    "eslint": "~4.9.0",
    "eslint-config-airbnb": "~16.0.0",
    "eslint-import-resolver-babel-module": "~4.0.0-beta.3",
    "eslint-plugin-import": "~2.7.0",
    "eslint-plugin-jsx-a11y": "~6.0.2",
    "eslint-plugin-react": "~7.4.0",
    "html-webpack-plugin": "~2.30.1",
    "jquery": "~3.2.0",
    "jsdom": "~11.3.0",
    "less": "~2.7.3",
    "less-loader": "~4.0.5",
    "mocha": "~4.0.1",
    "mugshot": "~0.3.2",
    "mugshot-looks-same": "~1.0.0",
    "mugshot-webdriverio": "~1.0.0",
    "nyc": "~11.3.0",
    "react": "^16.0.0",
    "react-cosmos-webpack": "^2.1.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "~3.0.0-beta.6",
    "sinon": "~4.1.1",
    "sinon-chai": "~2.14.0",
    "style-loader": "~0.19.0",
    "webdriverio": "~4.8.0",
    "webpack": "~3.7.1",
    "webpack-hot-middleware": "~2.19.1"
  }
}
